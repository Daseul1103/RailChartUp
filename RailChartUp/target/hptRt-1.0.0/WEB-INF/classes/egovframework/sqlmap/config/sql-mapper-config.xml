<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- jdb : 쿼리에 맞는 vo 설정 -->
	<typeAliases>
		<typeAlias alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap" />    
		<typeAlias alias="mainVO" type="kr.co.wizbrain.apRailroad.main.vo.MainVO" />    
		<typeAlias alias="statisticVO" type="kr.co.wizbrain.apRailroad.statistic.vo.StatisticVO" />    
		<typeAlias alias="logDataVO" type="kr.co.wizbrain.apRailroad.statistic.vo.LogDataVO" />    
		<typeAlias alias="mainStVo" type="kr.co.wizbrain.apRailroad.statistic.vo.MainStVo" />    
		<typeAlias alias="comuFailVO" type="kr.co.wizbrain.apRailroad.comuFail.vo.ComuFailVO" />    
		<typeAlias alias="terminalVO" type="kr.co.wizbrain.apRailroad.terminal.vo.TerminalVO" />    
		<typeAlias alias="userVO" type="kr.co.wizbrain.apRailroad.user.vo.UserVO" />    
		<typeAlias alias="dayCrowdRangeVO" type="kr.co.wizbrain.apRailroad.setting.vo.DayCrowdRangeVO" />    
