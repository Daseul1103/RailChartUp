		<typeAlias alias="list" type="java.util.List" />
		<typeAlias alias="map" type="java.util.Map" />
	</typeAliases>
	<!-- jdb : 사용하려는 *mapper.xml 등록 -->
	<mappers>
        <mapper resource="egovframework/sqlmap/mapper/mainMapper.xml" />
        <mapper resource="egovframework/sqlmap/mapper/statisticMapper.xml" />
        <mapper resource="egovframework/sqlmap/mapper/comuFailMapper.xml" />
        <mapper resource="egovframework/sqlmap/mapper/terminalMapper.xml" />
        <mapper resource="egovframework/sqlmap/mapper/userMapper.xml" />
        <mapper resource="egovframework/sqlmap/mapper/settingMapper.xml" />
   </mappers> 
 
</configuration>